*.zevross.dev {
    tls {
        dns cloudflare {env.DNS_PROVIDER_TOKEN}
    }

    @databeis host databeis.zevross.dev
    handle @databeis {
        reverse_proxy nextapp:3000
    }

    @databeisApi host api.databeis.zevross.dev
    handle @databeisApi {
        reverse_proxy goapp:8000
    }
}

:3000 {
    reverse_proxy nextapp:3000
}

:8000 {
    reverse_proxy goapp:8000
}
